{
  "name": "@eamonboyle/mssql-mcp",
  "version": "1.0.0",
  "description": "MCP server for Microsoft SQL Server - query and manage your database with natural language in Cursor, Claude, and VS Code",
  "keywords": [
    "mcp",
    "mssql",
    "sql-server",
    "database",
    "model-context-protocol",
    "cursor",
    "claude",
    "vscode"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eamonboyle/mssql-mcp.git"
  },
  "homepage": "https://github.com/eamonboyle/mssql-mcp#readme",
  "bugs": {
    "url": "https://github.com/eamonboyle/mssql-mcp/issues"
  },
  "main": "dist/index.js",
  "bin": {
    "mssql-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/mssql": "^9.1.7",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^10.0.0",
    "mssql": "^11.0.1"
  },
  "type": "module"
}
