{
  "mcp": {
    "servers": {
      "mssql": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@eamonboyle/mssql-mcp"],
        "env": {
          "SERVER_NAME": "localhost",
          "DATABASE_NAME": "YourDatabase",
          "DB_USER": "your_username",
          "DB_PASSWORD": "your_password",
          "READONLY": "false"
        }
      }
    }
  }
}
