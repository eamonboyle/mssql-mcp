{
  "mcp": {
    "servers": {
      "mssql": {
        "type": "stdio",
        "command": "node",
        "args": ["/path/to/mssql-mcp/dist/index.js"],
        "env": {
          "SERVER_NAME": "localhost",
          "DATABASE_NAME": "YourDatabase",
          "DB_USER": "your_username",
          "DB_PASSWORD": "your_password",
          "READONLY": "false"
        }
      }
    }
  }
}
